				$(this).parent().css('background-color', backgroundcolor);
				$(this).prev().css('color', color); 
			});

			$( 'a.menulevel1').hover(function() {
				/* Stuff to do when the mouse enters the element */
				$(this).css('color', '#fff'); 
			}, function() {
				/* Stuff to do when the mouse leaves the element */
				$(this).css('color', '#000');
			});

			// Add link to "tjeneste"
			$('.catnav > li').each(function(){
				var servicesrc = $(this).find('> a').attr('href');
				$(this).append('<a class="catnav-link" href="' + servicesrc + '">&rsaquo;</a>');
			});

		});

		function toggleMobileMenu(el) {
			console.log('touch on menubtn');
			$(el).parent().parent().find('ul.menu').slideToggle();
		}

		$( '.sidebar-mobile-btn' ).click(function(event) {
			/* Act on the event */
			$( '#sidemenu' ).slideToggle();
		});
	</script>
